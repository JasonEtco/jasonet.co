{
  "name": "jasonet.co",
  "private": true,
  "description": "https://jasonet.co",
  "author": "Jason Etcovitch <jasonetco@gmail.com>",
  "bugs": {
    "url": "https://github.com/JasonEtco/jasonet.co/issues"
  },
  "homepage": "https://jasonet.co",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write 'src/**/*.js'",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "dry": "npm run build && cp now.json public/ && cd public && now",
    "test": "jest && npm run lint"
  },
  "dependencies": {
    "gatsby": "^2.15.2",
    "gatsby-plugin-catch-links": "^2.1.7",
    "gatsby-plugin-feed": "^2.3.11",
    "gatsby-plugin-google-analytics": "^2.1.13",
    "gatsby-plugin-manifest": "^2.2.13",
    "gatsby-plugin-netlify": "^2.1.11",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.2.19",
    "gatsby-plugin-sitemap": "^2.2.10",
    "gatsby-plugin-typography": "^2.3.6",
    "gatsby-remark-autolink-headers": "^2.1.9",
    "gatsby-remark-copy-linked-files": "^2.1.14",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^3.1.20",
    "gatsby-remark-prismjs": "^3.3.12",
    "gatsby-remark-responsive-iframe": "^2.2.11",
    "gatsby-remark-smartypants": "^2.1.6",
    "gatsby-source-filesystem": "^2.1.19",
    "gatsby-transformer-remark": "^2.6.20",
    "gatsby-transformer-sharp": "^2.2.13",
    "inter-ui": "^3.9.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  },
  "devDependencies": {
    "@hapi/joi": "^15.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "front-matter": "^3.0.2",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.cache/"
    ]
  }
}
