{
  "name": "jasonet.co",
  "private": true,
  "description": "https://jasonet.co",
  "author": "Jason Etcovitch <jasonetco@gmail.com>",
  "bugs": {
    "url": "https://github.com/JasonEtco/jasonet.co/issues"
  },
  "homepage": "https://jasonet.co",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf _site",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:postcss": "TAILWIND_MODE=watch postcss assets/css/index.css -o _site/css/index.css --config ./postcss.config.js -w",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --quiet --incremental",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*",
    "prod:postcss": "postcss assets/css/index.css -o _site/css/index.css --config ./postcss.config.js",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "test": "jest && npm run lint",
    "compile-readme": "node bin/compile-readme.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@primer/octicons": "^16.2.0",
    "@tailwindcss/typography": "^0.5.0",
    "eleventy-plugin-time-to-read": "^1.1.1",
    "eleventy-plugin-toc": "^1.1.5",
    "nunjucks-date-filter": "^0.1.1",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "autoprefixer": "^10.4.0",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "joi": "^17.5.0",
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.cache/"
    ]
  }
}
