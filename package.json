{
  "name": "jasonet.co",
  "private": true,
  "description": "https://jasonet.co",
  "author": "Jason Etcovitch <jasonetco@gmail.com>",
  "bugs": {
    "url": "https://github.com/JasonEtco/jasonet.co/issues"
  },
  "homepage": "https://jasonet.co",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write 'src/**/*.js'",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "dry": "npm run build && cp now.json public/ && cd public && now",
    "test": "jest && npm run lint",
    "compile-readme": "node bin/compile-readme.js"
  },
  "dependencies": {
    "@primer/octicons-react": "^16.2.0",
    "@tailwindcss/typography": "^0.5.0",
    "gatsby": "^4.4.0",
    "gatsby-plugin-catch-links": "^4.4.0",
    "gatsby-plugin-feed": "^4.4.0",
    "gatsby-plugin-google-analytics": "^4.4.0",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-plugin-sitemap": "^5.4.0",
    "gatsby-remark-autolink-headers": "^5.4.0",
    "gatsby-remark-copy-linked-files": "^5.4.0",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^6.4.0",
    "gatsby-remark-prismjs": "^6.4.0",
    "gatsby-remark-responsive-iframe": "^5.4.0",
    "gatsby-remark-smartypants": "^5.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-remark": "^5.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "inter-ui": "^3.19.3",
    "joi": "^17.5.0",
    "prismjs": "^1.25.0",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^4.1.0",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "netlify-lambda": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.cache/"
    ]
  }
}
