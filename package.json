{
  "name": "jasonet.co",
  "private": true,
  "description": "https://jasonet.co",
  "author": "Jason Etcovitch <jasonetco@gmail.com>",
  "bugs": {
    "url": "https://github.com/JasonEtco/jasonet.co/issues"
  },
  "homepage": "https://jasonet.co",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write 'src/**/*.js'",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "dry": "npm run build && cp now.json public/ && cd public && now",
    "test": "jest && npm run lint",
    "compile-readme": "node bin/compile-readme.js"
  },
  "dependencies": {
    "@primer/octicons-react": "^16.2.0",
    "@tailwindcss/typography": "^0.5.0",
    "inter-ui": "^3.19.3",
    "prismjs": "^1.25.0",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "joi": "^17.5.0",
    "netlify-lambda": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.cache/"
    ]
  }
}
